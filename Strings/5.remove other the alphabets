// User function template for C++

class Solution {
  public:
    string removeSpecialCharacter(string s) {
        // code here
        string ans = "";
        for( int c : s){
            if(isalpha(c)){
                ans += c;
            }
            
        }
        if(ans.empty()){
            return "-1";
        }
        return ans;
    }
};
