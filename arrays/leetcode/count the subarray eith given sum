class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        
        int left=0;
        int count =0;
        int sum=0;
        for(int i=0;i<nums.size();i++){
            sum +=nums[i];
        
        while(sum>k && left<=i){
            sum -=nums[left];
            left++;
        }
        if(sum==k){
            count ++;
        }
        }
        return count;
    }
};
