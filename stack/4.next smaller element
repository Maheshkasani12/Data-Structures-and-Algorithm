#include <bits/stdc++.h>
using namespace std;

vector<int> nextSmaller(vector<int> &arr) {
    int n = arr.size();
    vector<int> nse(n, -1);
    stack<int> st;

    for(int i = n - 1; i >= 0; i--) {
        while(!st.empty() && st.top() >= arr[i])
            st.pop();

        if(!st.empty())
            nse[i] = st.top();

        st.push(arr[i]);
    }

    return nse;
}

int main() {
    int n;
    cin >> n;

    vector<int> arr(n);
    for(int i = 0; i < n; i++)
        cin >> arr[i];

    vector<int> ans = nextSmaller(arr);

    for(int x : ans)
        cout << x << " ";

    return 0;
}
